{
  "name": "noflo-wrapper",
  "version": "0.1.2",
  "description": "NoFlo component/graph testing and embedding wrapper compatible with BDD/TDD/whatever",
  "main": "index.js",
  "scripts": {
    "pretest": "./node_modules/.bin/coffeelint lib/*.coffee spec/*.coffee",
    "test": "./node_modules/.bin/mocha --compilers coffee:coffee-script/register --require coffee-script/register -R spec spec/*.coffee"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noflo/noflo-wrapper.git"
  },
  "keywords": [
    "noflo",
    "testing",
    "embedding"
  ],
  "author": "Vladimir Sibirov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noflo/noflo-wrapper/issues"
  },
  "homepage": "https://github.com/noflo/noflo-wrapper",
  "dependencies": {
    "bluebird": "^3.5.0",
    "coffee-script": "^1.12.0",
    "noflo": "0.x >= 0.8",
    "noflo-nodejs": "^0.8.1",
    "noflo-runtime-base": "^0.8.2"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "coffeelint": "^1.14.2",
    "mocha": "^3.2.0"
  }
}
