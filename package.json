{
  "name": "noflo-wrapper",
  "version": "0.2.1",
  "description": "NoFlo component/graph testing and embedding wrapper compatible with BDD/TDD/whatever",
  "main": "index.js",
  "scripts": {
    "pretest": "./node_modules/.bin/coffeelint lib/*.coffee spec/*.coffee",
    "test": "./node_modules/.bin/mocha --require coffeescript/register -R spec spec/*.coffee"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noflo/noflo-wrapper.git"
  },
  "keywords": [
    "noflo",
    "testing",
    "embedding"
  ],
  "author": "Vladimir Sibirov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noflo/noflo-wrapper/issues"
  },
  "homepage": "https://github.com/noflo/noflo-wrapper",
  "dependencies": {
    "bluebird": "^3.5.0",
    "coffeescript": "^2.0.2",
    "noflo": "^1.0.0",
    "noflo-nodejs": "^0.9.0",
    "noflo-runtime-base": "^0.9.0"
  },
  "devDependencies": {
    "chai": "^4.0.0",
    "coffeelint": "^2.0.7",
    "mocha": "^5.0.0"
  }
}
